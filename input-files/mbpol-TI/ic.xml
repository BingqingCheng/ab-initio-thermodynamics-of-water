<simulation verbosity="low">
    <output prefix="ic-273K-AW">
        <properties stride="1" filename="out">  [ step, time{picosecond}, conserved, temperature{kelvin}, potential, pot_component_raw(0), pot_component_raw(1), pressure_md{megapascal},volume] </properties>
